.microApplication {
  margin-top: 0px;
  height: 100%;
  margin-left: 0px;
  width: 100%;
}
.topView {
  width: 100%;
}
.topView-image {
  display: block;
  width: 100%; 
  height: 133px;
}

.collection-view {
  display: flex;
  margin-top: 10px;
}

.collection-cell-header {
  margin-left: 10px;
  text-align: left;
}

.collection-cell {
  position: relative;
  float: left;
  padding: 20px 10px;
  width: 25%;
  box-sizing: border-box;
}
.collection-cell-image {
  display:block; 
  margin:0 auto; 
  width: 40px;
  height: 40px;
}
.collection-cell-text {
  display:block;  
  margin:10px auto;  
  text-align:center;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  color: #AB99A1;
  font-size: 16px;
}

